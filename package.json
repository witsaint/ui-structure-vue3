{
  "name": "vite-admin",
  "version": "0.0.0",
  "scripts": {
    "dev": "concurrently \" npm run dev:md \" \" npm run dev:demo \" ",
    "dev:md": "vite  --mode md --config vite.md.config.js",
    "dev:demo": "vite --mode demos --config vite.demo.config.js ",
    "build:vite": "vite build",
    "build": "cross-env NODE_ENV=production webpack --config webpack.docs.js"
  },
  "dependencies": {
    "axios": "^0.20.0",
    "core-js": "^3.6.5",
    "vue": "^3.0.0-rc.1",
    "vue-router": "^4.0.0-0",
    "vuex": "^4.0.0-0"
  },
  "devDependencies": {
    "@vue/compiler-sfc": "^3.0.0-rc.1",
    "autoprefixer": "^10.0.1",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.2",
    "cssnano": "^4.1.10",
    "glob": "^7.1.6",
    "less": "^3.12.2",
    "less-loader": "^5.0.0",
    "lint-staged": "^9.5.0",
    "marked": "^1.2.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^4.0.1",
    "postcss-url": "^8.0.0",
    "precss": "^4.0.0",
    "vite": "^1.0.0-rc.1",
    "vite-plugin-vuedoc": "0.0.7",
    "webpack": "^5.1.3",
    "webpack-cli": "^4.1.0"
  }
}
